@typeparam TGridItem
@using Microsoft.AspNetCore.Components.Rendering
@inherits RowBase<TGridItem>

@code
{
    /// <summary>
    /// Template to render as the row content
    /// </summary>
    [Parameter] public RenderFragment ChildContent { get; set; }
    
    public override void RenderContent(RenderTreeBuilder builder)
    {
        ChildContent.Invoke(builder);
    }
}
